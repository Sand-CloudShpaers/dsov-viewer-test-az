{
  "name": "dso-viewer",
  "version": "1.20.0-SNAPSHOT",
  "license": "CTI",
  "scripts": {
    "postinstall": "ngcc",
    "ng": "ng",
    "prettier": "prettier --config ./.prettierrc.js --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "start": "ng serve --proxy-config ./src/config/proxy.conf.json",
    "build": "ng build --configuration production --source-map=false",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --karma-config ./karma-ci.conf.js --code-coverage",
    "test:wsl": "ng test --karma-config ./karma-wsl.conf.js",
    "lint": "eslint -c .eslintrc.js --ext .ts,.component.html ./src/app && tsc --noEmit",
    "lint:ci": "eslint -c .eslintrc.js --ext .ts,.component.html ./src/app -f checkstyle -o checkstyle-result.xml",
    "generate-openapi-model-presenteren": "openapi-generator-cli generate -i https://nep-knooppunt-ketenacceptatie.viewer.dso.kadaster.nl/publiek/omgevingsdocumenten/api/presenteren/v7/openapi.json -g typescript-angular -o ./src/app/model/swagger/ozon_presenteren_v7 --global-property models",
    "generate-openapi-model-verbeelden": "openapi-generator-cli generate -i https://nep-knooppunt-ketenacceptatie.viewer.dso.kadaster.nl/publiek/omgevingsdocumenten/api/verbeelden/v3/openapi.json -g typescript-angular -o ./src/app/model/swagger/ozon_verbeelden_v3 --global-property models",
    "generate-openapi-model-ihr": "openapi-generator-cli generate -i https://ruimte.omgevingswet.overheid.nl/ruimtelijke-plannen/api/opvragen/v4/ -g typescript-angular -o ./src/app/model/swagger/ihr_v4 --global-property models",
    "xslt:ps-svg": "xslt3 -xsl:./src/generation/assets/xslt/puntsymbolen-svg.xslt -s:./src/generation/assets/source/puntsymbolen/SLD_Symbolenbibliotheek_STOPTPOD_Puntsymbolen_v1.3.0.sld",
    "xslt:ls-svg": "xslt3 -xsl:./src/generation/assets/xslt/lijnsymbolen-svg.xslt -s:./src/generation/assets/source/lijnsymbolen/SLD_Symbolenbibliotheek_STOPTPOD_Lijnsymbolen_v1.3.0.sld",
    "xslt:ps-scss": "xslt3 -xsl:./src/generation/assets/xslt/scss.xslt -s:./src/generation/assets/source/puntsymbolen/SLD_Symbolenbibliotheek_STOPTPOD_Puntsymbolen_v1.3.0.sld -o:./src/assets/symbolen/dso-symbolen-punten.scss type=punt && prettier --config ./.prettierrc.js --write \"**/dso-symbolen-punten.scss\"",
    "xslt:ls-scss": "xslt3 -xsl:./src/generation/assets/xslt/scss.xslt -s:./src/generation/assets/source/lijnsymbolen/SLD_Symbolenbibliotheek_STOPTPOD_Lijnsymbolen_v1.3.0.sld -o:./src/assets/symbolen/dso-symbolen-lijnen.scss type=lijn && prettier --config ./.prettierrc.js --write \"**/dso-symbolen-lijnen.scss\"",
    "xslt:ps-legend": "xslt3 -xsl:./src/generation/assets/xslt/legenda.xslt -s:./src/generation/assets/source/puntsymbolen/SLD_Symbolenbibliotheek_STOPTPOD_Puntsymbolen_v1.3.0.sld -o:./src/generation/assets/legenda/puntsymbolen.html type=punt",
    "xslt:ls-legend": "xslt3 -xsl:./src/generation/assets/xslt/legenda.xslt -s:./src/generation/assets/source/lijnsymbolen/SLD_Symbolenbibliotheek_STOPTPOD_Lijnsymbolen_v1.3.0.sld -o:./src/generation/assets/legenda/lijnsymbolen.html type=lijn"
  },
  "engines": {
    "node": "^14.15.0 || >=16.10.0",
    "npm": ">=6.11.0 <=8"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.0.4",
    "@angular/cdk": "14.0.3",
    "@angular/common": "14.0.4",
    "@angular/compiler": "14.0.4",
    "@angular/core": "14.0.4",
    "@angular/forms": "14.0.4",
    "@angular/material": "14.0.3",
    "@angular/platform-browser": "14.0.4",
    "@angular/platform-browser-dynamic": "14.0.4",
    "@angular/router": "14.0.4",
    "@angular/service-worker": "14.0.4",
    "@dso-toolkit/core": "46.0.0",
    "@ngrx/effects": "14.0.0",
    "@ngrx/entity": "14.0.0",
    "@ngrx/router-store": "14.0.0",
    "@ngrx/store": "14.0.0",
    "@turf/helpers": "^6.5.0",
    "@turf/kinks": "^6.5.0",
    "@types/mapbox-gl": "^2.7.5",
    "dso-toolkit": "46.0.0",
    "element-closest": "^3.0.2",
    "focus-visible": "^5.2.0",
    "iframe-resizer": "^4.3.2",
    "jsts": "2.6.2",
    "lodash-es": "^4.17.21",
    "ol": "6.14.1",
    "ol-mapbox-style": "7.1.1",
    "rxjs": "^7.5.5",
    "striptags": "^3.2.0",
    "tslib": "^2.4.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.4",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "14.0.4",
    "@angular/compiler-cli": "14.0.4",
    "@angular/language-service": "14.0.4",
    "@ngneat/spectator": "^11.1.0",
    "@ngrx/store-devtools": "14.0.0",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/iframe-resizer": "^3.5.9",
    "@types/jasmine": "~4.0.3",
    "@types/lodash-es": "^4.17.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jasmine": "^4.1.3",
    "eslint-plugin-rxjs": "^5.0.2",
    "eslint-plugin-rxjs-angular": "^2.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jasmine-core": "~4.2.0",
    "jasmine-marbles": "~0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-sabarivka-reporter": "^3.3.1",
    "karma-spec-reporter": "0.0.34",
    "ng-mocks": "^14.0.1",
    "prettier": "^2.7.1",
    "puppeteer": "^15.3.0",
    "typescript": "4.7.4",
    "xslt3": "^2.4.0"
  }
}
